  },
  "description": "Lazy Trainer is a web application that allows you to create and manage your workouts and exercises. It is also possible to create and manage your own training plans.",
  "repository": {
    "type": "git",
    "url": "git@github.com:simone98dm/lazy-trainer-fe.git"
  },
  "license": "MIT",
  "version": "1.68.4",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "vercel:deploy": "vercel deploy",
    "vercel:run": "vercel dev",
    "postinstall": "nuxt prepare",
    "mock": "npm --prefix mock run start",
    "lint": "prettier -w . && eslint --ext .ts,.vue --fix --ignore-path .gitignore .",
    "test:unit": "vitest run --config vitest.config.ts --dom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
