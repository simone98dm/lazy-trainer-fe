{
  "name": "lazy-trainer-frontend",
  "private": true,
  "version": "1.63.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "vercel:dev": "vercel dev",
    "test:e2e": "npx cypress open",
    "mock": " node ./local/index.js",
    "prepare": "husky install",
    "gource": "gource -f -s 1 -a 1 --key --max-files 0 --disable-progress",
    "release:patch": "release-it -- patch --ci ",
    "release:minor": "release-it -- minor --ci ",
    "release:major": "release-it -- major --ci "
  },
  "dependencies": {
    "@cypress/vue": "^3.1.0",
    "@cypress/webpack-dev-server": "^2.3.0",
    "@logtail/node": "^0.1.14",
    "@material-design-icons/svg": "^0.13.0",
    "bcrypt": "^5.0.1",
    "cypress": "^10.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "firebase": "^9.12.1",
    "idb-keyval": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "material-icons": "^1.12.0",
    "mongodb": "^4.10.0",
    "pinia": "^2.0.22",
    "postgres": "^3.3.1",
    "uuid": "^9.0.0",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.37",
    "vue-hotjar": "^1.4.0",
    "vue-router": "^4.1.5",
    "vue-svg-loader": "^0.16.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vercel/node": "^2.5.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.9",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "release-it": "^15.5.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.0",
    "vite-plugin-fonts": "^0.6.0",
    "vite-plugin-html-config": "^1.0.10",
    "vite-plugin-pwa": "^0.13.1",
    "vue-tsc": "^0.40.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  }
}
