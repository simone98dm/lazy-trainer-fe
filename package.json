    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.11.1",
    "express": "^4.18.2",
    "happy-dom": "^13.2.1",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "nuxt": "^3.9.3",
