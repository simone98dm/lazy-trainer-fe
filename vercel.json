{
    "name": "lazy-trainer-api",
    "version": 1,
    "public": true,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "source": "/api/(.*)",
            "dest": "/api/server.ts"
        }
    ]
}